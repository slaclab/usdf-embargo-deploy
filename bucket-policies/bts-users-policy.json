{
  "Version": "2012-10-17",
  "Statement": [{
    "Sid": "bts-users-policy",
    "Effect": "Allow",
    "Principal": {"AWS": ["arn:aws:iam:::user/rubin-summit-users"]},
    "Action": [
      "s3:DeleteObject",
      "s3:DeleteObjectVersion",
      "s3:GetBucketLocation",
      "s3:GetObject",
      "s3:GetObjectVersion",
      "s3:ListBucket",
      "s3:ListBucketVersions",
      "s3:PutObject"
    ],
    "Resource": [
      "arn:aws:s3:::rubin-bts-users",
      "arn:aws:s3:::rubin-bts-users/*"
    ]
  }]
}
